# Plaid Configuration
PLAID_ENV=development
PLAID_CLIENT_ID=__YOUR_ID__
PLAID_SECRET=__YOUR_SECRET__
PLAID_COUNTRY_CODES=CA,US
PLAID_PRODUCTS=transactions
# Required for iOS OAuth institutions (must exactly match Plaid Dashboard redirect URI)
# Example: https://yourdomain.com/plaid/oauth-return
PLAID_REDIRECT_URI=
# Optional: Link token webhook URL
PLAID_WEBHOOK_URL=
# Optional: Android native package name if using native Android Link SDK
PLAID_ANDROID_PACKAGE_NAME=

# Server Configuration
BACKEND_PORT=8000
FRONTEND_PORT=3000
DATABASE_URL=sqlite:///./app.db

# Security
SECRET_KEY=your-secret-key-for-encryption
APP_SHARED_KEY=replace-with-a-long-random-secret








